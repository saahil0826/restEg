<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>products</title>
  </head>
  <body>
    <ul>
      <% products.forEach(function(p){ %>
      <li><b><%=p.name %></b></li>
      <a href="/products/<%=p._id %>">more info</a>
      <% }) %>
    </ul>
    <a href="/products/new">Add a new product</a>
  </body>
</html>
